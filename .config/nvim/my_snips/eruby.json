{
  "link_to": {
    "prefix": "lt",
    "body": [
      "<%= link_to $1 %>"
    ],
    "description": "link_to .. "
  },
  "link_block": {
    "prefix": "ltb",
    "body": [
      "<% link_to ${1:path} do %>",
      "  $2",
      "<% end %>"
    ],
    "description": "link_to with block"
  },
  "render": {
    "prefix": "rp",
    "body": [
      "<%= render $1 %>"
    ],
    "description": "render partial"
  },
}
